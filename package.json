{
  "name": "example-graphql-server",
  "version": "1.0.2",
  "description": "Example of a node server with a GraphQL endpoint",
  "scripts": {
    "start": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "startWindows": "SET NODE_ENV=development&&nodemon src/index.js --exec babel-node",
    "build": "babel src --ignore __tests__ --out-dir dist/",
    "serve": "sudo NODE_ENV=production node dist/index.js",
    "serveWindows": "SET NODE_ENV=production&&node dist/index.js",
    "test": "mocha --require resources/mochaSetup src/**/__tests__/**/*.js",
    "check": "flow check",
    "lint": "standard",
    "travis": "npm run check && npm run lint && npm run test"
  },
  "engines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiThereCommunity/example-graphql-server.git"
  },
  "keywords": [
    "todo-list",
    "graphQL",
    "API",
    "node.js"
  ],
  "author": "Dirk-Jan Rutten <dirkjanrutten@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HiThereCommunity/example-graphql-server/issues"
  },
  "homepage": "https://github.com/HiThereCommunity/example-graphql-server",
  "dependencies": {
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "flow-bin": "^0.27.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "standard": "^8.0.0"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
