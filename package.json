{
  "name": "example-graphql-server",
  "version": "1.0.0",
  "description": "Example of a node server with a GraphQL endpoint",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "build": "babel src --ignore __tests__ --out-dir dist/",
    "serve": "node dist/index.js",
    "test": "mocha --require resources/MochaSetup src/**/__tests__/**/*.js || exit 0",
    "check": "flow check || exit 0"
  },
  "engines": {
    "node": ">=5.3.0",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiThereCommunity/example-graphql-server.git"
  },
  "keywords": [
    "todo-list",
    "graphQL",
    "API",
    "node.js"
  ],
  "contributors": [
    "Dirk-Jan Rutten <dirk-jan.rutten@hi-there.community>",
    "Florentijn Hogerwerf <florentijn.hogerwerf@hi-there.community>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HiThereCommunity/example-graphql-server/issues"
  },
  "homepage": "https://github.com/HiThereCommunity/example-graphql-server",
  "dependencies": {
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "flow-bin": "^0.27.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2"
  }
}
